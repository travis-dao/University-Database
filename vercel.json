{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "headers": [
        {
        "source": "/(.*)",
        "headers": [
            {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
            }
        ]
        }
    ]
}